            "hash": "de62f35b01b577db29d75cc8b6807527fce56827b3a4a9f359f7d6dfac5c04fa"
        },
        "arm64": {
            "url": "https://github.com/jdx/mise/releases/download/v2025.5.17/mise-v2025.5.17-windows-arm64.zip",
