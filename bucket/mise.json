            "hash": "65c9d62e6245db3fe8d728235d053d6e4df17a0050d79dd58c4b0b2e604bc315"
        },
        "arm64": {
            "url": "https://github.com/jdx/mise/releases/download/v2025.9.5/mise-v2025.9.5-windows-arm64.zip",
